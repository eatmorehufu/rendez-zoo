<section class="event-details-main">
  <h1><%= groupEvent.escape("title") %></h1>
  <h3><%= groupEvent.escape("start_time") %> <%= groupEvent.escape("end_time") %></h3>
  <div id="event-location">Google Map</div>
  <p><%= groupEvent.escape("description") %></p>
</section>

<section class="event-details-sidebar">
  <button class="rsvp-button">Join/RSVP</button>
  <h3><%= groupEvent.attendees().length %> Attending</h3>
  <div id="event-attendee-profiles">Attendees</div>
</section>
