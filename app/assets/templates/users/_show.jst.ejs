<main class="entire-user-profile group-subpage group">
  <section class="user-profile-main">
    <h1> <%= user.get('username') %></h1>
      <h3><strong>Location:</strong><%= user.escape('city') %>, <%= user.escape('state') %></h3>
      <h3><strong>Rendez-Zoo member since: </strong><%= timeParse.monthYear %></h3>
      <h3><strong>Bio:</strong></h3>
      <p><%=user.escape('description') %></p>

      <article>
        <h2>Member Of: </h2>
        <ul class="group-memberships">
        </ul>
        <h2>Organizer for: </h2>
        <ul class="organizer-memberships">
        </ul>
      </article>
    </section>


    <section class="user-profile-sidebar">
      <div class="profile-picture">
        <img src="<%= user.escape("avatar_url") %>">
      </div>


      <div class="user-interests">
        <h2>Interested In: </h2>
        <ul>
          <% user.interests().forEach(function(interest){ %>
            <li><%= interest.escape('name') %></li>
          <% })%>
        </ul>
      </div>
    </section>
</main>
